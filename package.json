{
  "name": "proshop",
  "version": "1.0.0",
  "description": "eCommerce App which built with MERN Stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hnnhack/webshop.git"
  },
  "author": "hnnhack",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hnnhack/webshop/issues"
  },
  "homepage": "https://github.com/hnnhack/webshop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "express": "^4.18.2",
    "mongoose": "^8.1.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.2",
    "nodemon": "^3.0.3"
  }
}
